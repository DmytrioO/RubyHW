<p style="color: green"><%= notice %></p>

<h1>Quotes</h1>

<%= turbo_frame_tag "second_frame" do %>
  <div id="quotes">
    <% @quotes.each do |quote| %>
      <%= render quote %>
      <p>
        <%= link_to "Show this quote", quote %>
        <%= button_to "Destroy this quote", quote, method: :delete %>
      </p>
    <% end %>
  </div>
<% end %>

<%= turbo_frame_tag "first_turbo_frame" do %>
  <%= link_to "New quote", new_quote_path, data: { turbo_frame: "second_frame" } %>
<% end %>